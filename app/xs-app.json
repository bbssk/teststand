{
  "welcomeFile": "teststandui/webapp/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/app-logout",
    "logoutPage": "/"
  },
  "routes": [
    {
      "source": "^/teststandui/webapp/(.*)$",
      "target": "$1",
      "localDir": "teststandui/webapp/",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": true
    }
  ]
}
